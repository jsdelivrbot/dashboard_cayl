@font-face {
  font-family: "icon";
  src: url( "/fonts/icon.eot" );
  src: url( "/fonts/icon.eot?#iefix" ) format( "eot" ),
       url( "/fonts/icon.woff" ) format( "woff" ),
       url( "/fonts/icon.ttf" ) format( "truetype" ),
       url( "/fonts/icon.svg#icon" ) format( "svg" );
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
  font-family: "icon";
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  speak: none;
  display: inline-block;

  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-icon1:before { content: "\EA01" }
.icon-icon2:before { content: "\EA02" }
.icon-icon3:before { content: "\EA03" }
.icon-icon4:before { content: "\EA04" }
.icon-logo1:before { content: "\EA05" }
.icon-logo2:before { content: "\EA06" }
.icon-logo3:before { content: "\EA07" }